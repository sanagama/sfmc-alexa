  },
  "homepage": "https://github.com/sanagama/sfmc-alexa#readme",
  "devDependencies": {
    "@types/shortid": "0.0.29",
    "@types/node": "^10.5.2",
    "ts-node": "^7.0.1",
    "gulp": "^3.9.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "typescript": "^3.1.3",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "ask-sdk": "^2.1.0",
